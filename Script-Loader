local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local player = Players.LocalPlayer


local gui = Instance.new("ScreenGui")
gui.Name = "override.cc | Script Loader"
gui.ResetOnSpawn = false
gui.Parent = player:WaitForChild("PlayerGui")


local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0,600,0,450)
mainFrame.Position = UDim2.new(0.5,-300,0.5,-225)
mainFrame.BackgroundColor3 = Color3.fromRGB(20,20,20)
mainFrame.BorderSizePixel = 0
mainFrame.Active = true
mainFrame.Draggable = true
mainFrame.Parent = gui

local mainCorner = Instance.new("UICorner")
mainCorner.CornerRadius = UDim.new(0,20)
mainCorner.Parent = mainFrame


local title = Instance.new("TextLabel")
title.Text = "override.cc"
title.Size = UDim2.new(1,0,0,50)
title.Position = UDim2.new(0,0,0,0)
title.BackgroundTransparency = 1
title.Font = Enum.Font.GothamBlack
title.TextSize = 36
title.TextColor3 = Color3.fromRGB(255,255,255)
title.TextStrokeTransparency = 1
title.Parent = mainFrame


local function createButton(parent, pos, text, color)
    local btn = Instance.new("TextButton")
    btn.Size = UDim2.new(0,35,0,35)
    btn.Position = pos
    btn.Text = text
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 24
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.BackgroundColor3 = color
    btn.BorderSizePixel = 0
    btn.Parent = parent
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0,6)
    corner.Parent = btn
    return btn
end

local closeBtn = createButton(mainFrame, UDim2.new(1,-40,0,5), "X", Color3.fromRGB(200,0,0))
local minimizeBtn = createButton(mainFrame, UDim2.new(1,-80,0,5), "-", Color3.fromRGB(255,100,100))


local scroll = Instance.new("ScrollingFrame")
scroll.Size = UDim2.new(1,-20,1,-120)
scroll.Position = UDim2.new(0,10,0,60)
scroll.BackgroundTransparency = 1
scroll.ScrollBarThickness = 6
scroll.ScrollBarImageColor3 = Color3.fromRGB(255,0,0)
scroll.CanvasSize = UDim2.new(0,0,0,0)
scroll.Parent = mainFrame

local listLayout = Instance.new("UIListLayout")
listLayout.Padding = UDim.new(0,16)
listLayout.SortOrder = Enum.SortOrder.LayoutOrder
listLayout.Parent = scroll


local statusLabel = Instance.new("TextLabel")
statusLabel.Text = "Status: ðŸŸ¢ = Online ðŸ”´ = Offline"
statusLabel.Size = UDim2.new(0,300,0,30)
statusLabel.Position = UDim2.new(0,10,1,-35)
statusLabel.BackgroundTransparency = 1
statusLabel.Font = Enum.Font.Gotham
statusLabel.TextSize = 14
statusLabel.TextColor3 = Color3.fromRGB(255,255,255)
statusLabel.TextXAlignment = Enum.TextXAlignment.Left
statusLabel.Parent = mainFrame

local function CreateScriptEntry(name, desc, callback)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1,-10,0,110)
    frame.BackgroundColor3 = Color3.fromRGB(30,30,30)
    frame.BorderSizePixel = 0
    frame.Parent = scroll

    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0,14)
    corner.Parent = frame

    
    local scriptName = Instance.new("TextLabel")
    scriptName.Text = name
    scriptName.Size = UDim2.new(1,-20,0,30)
    scriptName.Position = UDim2.new(0,10,0,5)
    scriptName.Font = Enum.Font.GothamBlack
    scriptName.TextSize = 20
    scriptName.TextColor3 = Color3.fromRGB(255,255,255)
    scriptName.BackgroundTransparency = 1
    scriptName.Parent = frame

    
    local descLabel = Instance.new("TextLabel")
    descLabel.Text = desc
    descLabel.Size = UDim2.new(1,-20,0,40)
    descLabel.Position = UDim2.new(0,10,0,35)
    descLabel.BackgroundTransparency = 1
    descLabel.Font = Enum.Font.Gotham
    descLabel.TextSize = 14
    descLabel.TextColor3 = Color3.fromRGB(200,200,200)
    descLabel.TextWrapped = true
    descLabel.TextXAlignment = Enum.TextXAlignment.Left
    descLabel.Parent = frame

    
    local loadBtn = Instance.new("TextButton")
    loadBtn.Size = UDim2.new(0,120,0,35)
    loadBtn.Position = UDim2.new(1,-130,0,40)
    loadBtn.Text = "LOAD"
    loadBtn.Font = Enum.Font.GothamBold
    loadBtn.TextSize = 16
    loadBtn.TextColor3 = Color3.fromRGB(255,255,255)
    loadBtn.BackgroundColor3 = Color3.fromRGB(180,0,0)
    loadBtn.BorderSizePixel = 0
    loadBtn.Parent = frame

    local btnCorner = Instance.new("UICorner")
    btnCorner.CornerRadius = UDim.new(0,6)
    btnCorner.Parent = loadBtn

    loadBtn.MouseEnter:Connect(function()
        TweenService:Create(loadBtn,TweenInfo.new(0.15),{BackgroundColor3=Color3.fromRGB(255,50,50)}):Play()
    end)
    loadBtn.MouseLeave:Connect(function()
        TweenService:Create(loadBtn,TweenInfo.new(0.15),{BackgroundColor3=Color3.fromRGB(180,0,0)}):Play()
    end)

    loadBtn.MouseButton1Click:Connect(function()
        
        local progressGui = Instance.new("ScreenGui")
        progressGui.Name = "ProgressGui"
        progressGui.Parent = player:WaitForChild("PlayerGui")

        local progressFrame = Instance.new("Frame")
        progressFrame.Size = UDim2.new(0,400,0,120)
        progressFrame.Position = UDim2.new(0.5,-200,0.5,-60)
        progressFrame.BackgroundColor3 = Color3.fromRGB(20,20,20)
        progressFrame.BorderSizePixel = 0
        progressFrame.Parent = progressGui

        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0,15)
        corner.Parent = progressFrame

        local titleLabel = Instance.new("TextLabel")
        titleLabel.Text = "Initializing..."
        titleLabel.Size = UDim2.new(1,0,0,30)
        titleLabel.Position = UDim2.new(0,0,0,10)
        titleLabel.BackgroundTransparency = 1
        titleLabel.Font = Enum.Font.GothamBold
        titleLabel.TextSize = 18
        titleLabel.TextColor3 = Color3.fromRGB(255,255,255)
        titleLabel.Parent = progressFrame

        local percentLabel = Instance.new("TextLabel")
        percentLabel.Text = "00.00%"
        percentLabel.Size = UDim2.new(1,0,0,25)
        percentLabel.Position = UDim2.new(0,0,0,40)
        percentLabel.BackgroundTransparency = 1
        percentLabel.Font = Enum.Font.Gotham
        percentLabel.TextSize = 16
        percentLabel.TextColor3 = Color3.fromRGB(255,255,255)
        percentLabel.Parent = progressFrame

        local barBack = Instance.new("Frame")
        barBack.Size = UDim2.new(1,-40,0,20)
        barBack.Position = UDim2.new(0,20,0,75)
        barBack.BackgroundColor3 = Color3.fromRGB(50,50,50)
        barBack.BorderSizePixel = 0
        barBack.Parent = progressFrame
        local backCorner = Instance.new("UICorner")
        backCorner.CornerRadius = UDim.new(0,10)
        backCorner.Parent = barBack

        local barFill = Instance.new("Frame")
        barFill.Size = UDim2.new(0,0,1,0)
        barFill.Position = UDim2.new(0,0,0,0)
        barFill.BackgroundColor3 = Color3.fromRGB(180,0,0)
        barFill.BorderSizePixel = 0
        barFill.Parent = barBack
        local fillCorner = Instance.new("UICorner")
        fillCorner.CornerRadius = UDim.new(0,10)
        fillCorner.Parent = barFill

        
        local steps = {
            {text = "Initializing Scripts", duration = 2},
            {text = "Injecting Hooks", duration = 2},
            {text = "Finalizing Execution", duration = 2},
            {text = "Script loaded successfully!", duration = 1}
        }

        
        local totalTime = 0
        for _, step in ipairs(steps) do totalTime = totalTime + step.duration end
        local elapsed = 0

        spawn(function()
            for _, step in ipairs(steps) do
                titleLabel.Text = step.text
                local stepStart = elapsed
                local stepEnd = elapsed + step.duration

                local goal = {Size = UDim2.new(stepEnd/totalTime,0,1,0)}
                local tween = TweenService:Create(barFill, TweenInfo.new(step.duration, Enum.EasingStyle.Linear), goal)
                tween:Play()

                local startTime = tick()
                while tick() - startTime < step.duration do
                    local currentPercent = math.clamp(((elapsed + (tick()-startTime))/totalTime)*100,0,100)
                    percentLabel.Text = string.format("%.2f%%", currentPercent)
                    RunService.RenderStepped:Wait()
                end

                elapsed = elapsed + step.duration
            end

            percentLabel.Text = "100.00%"
            barFill.Size = UDim2.new(1,0,1,0)
            wait(0.5)
            progressGui:Destroy()
            callback()
        end)

        gui:Destroy()
    end)
end


CreateScriptEntry("Tha Bronx 3 ðŸŸ¢","Infinite Money, Fast Gun Dupe, Instant Teleports", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/striketoolit/override.cc-TB3/refs/heads/main/Luarmor"))()
end)
CreateScriptEntry("South Bronx ðŸ”´","Autofarms, Silent + Gun Mods, Instant Teleports", function()
    print("South Bronx | Coming soon.")
end)
CreateScriptEntry("Philly Streets 2 ðŸ”´","Players Options, Money Generator, Dupe", function()
    print("Philly Streets 2 | Coming soon.")
end)


local minimized = false
minimizeBtn.MouseButton1Click:Connect(function()
    minimized = not minimized
    TweenService:Create(mainFrame, TweenInfo.new(0.25), {
        Size = minimized and UDim2.new(0,600,0,60) or UDim2.new(0,600,0,450)
    }):Play()
    scroll.Visible = not minimized
end)

closeBtn.MouseButton1Click:Connect(function()
    gui:Destroy()
end)
